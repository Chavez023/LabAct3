<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--Using Relative Layout-->
    <style name="rel.Top_part" parent="TextAppearance.AppCompat">
        <item name="android:paddingTop">40dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="rel.Mid_part" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_below">@id/Header</item>
        <item name="android:layout_above">@+id/bottom_navigation</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">20dp</item>
    </style>
    <style name="rel.Bot.Nav_part" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">20dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="rel.Top.Status_part" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_below">@id/Header</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <!--Text-->
    <style name="Header.Left_text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/noto_sans</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/forhead</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="Header.Center_text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/noto_sans</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/forhead</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="Description.Center_text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/noto_sans</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#BBBBBB</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="label_text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/noto_sans</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="step_true.text" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/o_status_steps_true</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="step_false.text" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/o_status_steps_false</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <!--Fields-->
    <style name="Input.norm_field" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@drawable/o_unibox</item>
        <item name="android:textColorHint">#BBBBBB</item>
        <item name="android:textColor">#720000</item>
        <item name="android:ems">10</item>
    </style>
    <style name="Input.maxwidth_field" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@drawable/o_unibox</item>
        <item name="android:textColorHint">#BBBBBB</item>
        <item name="android:textColor">#720000</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:ems">10</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="radio_button.field" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--Form-->




    <!--Nav-->

    <style name="iset_Nav_items_H" parent="TextAppearance.AppCompat">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>

    </style>
    <!--Items-->



    <!--Button-->
    <style name="But.norm.button" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#720000</item>
        <item name="android:radius">0dp</item>

    </style>


    <style name="quick.Tools_dash.widg" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="background">@drawable/o_unibox</item>
        <item name="android:padding">10dp</item>
    </style>

<!--container-->
    <style name="icon.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="icon_text.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="item_Horizontal.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="item_Vertical.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="radio_group_Vertical.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="radio_group_Horizontal.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="sv.holder_Vertical.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="form_Single.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">20dp</item>
        <item name="android:background">@drawable/o_unibox</item>
    </style>
    <style name="form_Multi.con" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">20dp</item>
        <item name="android:background">@drawable/o_unibox</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>



</resources>